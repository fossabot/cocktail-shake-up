{
  "name": "cocktail-shake-up",
  "version": "0.1.1",
  "private": true,
  "browserslist": "> 1%, last 2 versions",
  "devDependencies": {
    "@percy-io/percy-storybook": "^1.2.3",
    "@storybook/addon-actions": "^3.2.11",
    "@storybook/addon-knobs": "^3.2.10",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/react": "^3.2.11",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "codecov": "^2.3.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.0",
    "empty": "^0.10.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.0.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.0.0",
    "flow-bin": "^0.55.0",
    "flow-coverage-report": "^0.3.0",
    "greenkeeper-lockfile": "^1.9.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.0.2",
    "json-loader": "0.5.7",
    "netlify-cli": "^1.2.2",
    "postcss-loader": "^2.0.3",
    "prettier": "^1.7.1",
    "react-dev-utils": "^4.0.0",
    "react-test-renderer": "^16.0.0",
    "snyk": "^1.34.4",
    "storybook-addon-intl": "^2.1.1",
    "style-loader": "^0.18.2",
    "stylelint": "^8.1.1",
    "url-loader": "^0.6.1",
    "wdio-concise-reporter": "^0.1.2",
    "wdio-mocha-framework": "^0.5.9",
    "wdio-sauce-service": "^0.4.0",
    "wdio-selenium-standalone-service": "^0.0.9",
    "webdriverio": "^4.6.2",
    "webpack": "^3.0.0",
    "webpack-dashboard": "^1.0.0-3",
    "webpack-dev-server": "^2.4.3"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "date-fns": "^1.28.5",
    "localforage": "^1.5.0",
    "material-design-icons": "^3.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.0.3",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rollbar": "^2.2.9"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --colors --config config/webpack.config.prod.js",
    "coverage": "flow-coverage-report -i 'app/**/*.js' -x 'app/**/*.stories.js'; jest --all --coverage",
    "deploy": "netlify --access-token $NETLIFY_SECRET deploy",
    "lint": "yarn prettier; stylelint 'app/**/*.css' --fix; eslint './+(app|webdriver)/**/*.js' --fix; flow",
    "prettier": "prettier './+(app|webdriver)/**/*.+(css|js)' --write",
    "sauce": "wdio webdriver/wdio.sauce.js",
    "snapshot": "build-storybook && percy-storybook",
    "start": " cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js",
    "storybook": "start-storybook -p 6006",
    "test": "snyk test; jest; codecov",
    "types": "flow-typed install",
    "webdriver": "wdio webdriver/wdio.conf.js",
    "postinstall": "snyk protect"
  },
  "babel": {
    "env": {
      "development": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      },
      "production": {
        "presets": [
          "minify"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    },
    "presets": [
      [
        "env",
        {
          "modules": false,
          "spec": true,
          "useBuiltIns": true
        }
      ],
      "flow",
      "react"
    ],
    "plugins": [
      [
        "react-intl",
        {
          "messagesDir": "./messages"
        }
      ],
      "transform-do-expressions",
      [
        "transform-class-properties",
        {
          "spec": true
        }
      ],
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true,
      "serviceworker": true,
      "worker": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:flowtype/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jest/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2016,
      "sourceType": "module"
    },
    "plugins": [
      "flowtype",
      "import",
      "jest",
      "jsx-a11y",
      "react"
    ],
    "rules": {
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "brace-style": [
        "error",
        "1tbs"
      ],
      "camelcase": "error",
      "curly": "error",
      "default-case": "error",
      "dot-notation": "error",
      "eqeqeq": "error",
      "keyword-spacing": "error",
      "no-console": "warn",
      "no-eval": "error",
      "no-param-reassign": "error",
      "no-useless-constructor": "error",
      "no-var": "error",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "object-property-newline": "error",
      "object-shorthand": [
        "error",
        "always"
      ],
      "one-var": [
        "error",
        "never"
      ],
      "prefer-const": "error",
      "prefer-rest-params": "error",
      "prefer-spread": "error",
      "prefer-template": "warn",
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "semi": "error",
      "sort-imports": [
        "warn",
        {
          "memberSyntaxSortOrder": [
            "all",
            "multiple",
            "single",
            "none"
          ]
        }
      ],
      "yoda": [
        "error",
        "never",
        {
          "exceptRange": true
        }
      ]
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.js",
      "!app/**/*.stories.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "notify": false,
    "setupFiles": [
      "./config/setup.js"
    ],
    "testPathIgnorePatterns": [
      "webdriver/"
    ]
  },
  "prettier": {
    "bracket-spacing": true,
    "jsx-bracket-same-line": false,
    "print-width": 120,
    "semi": true,
    "single-quote": true,
    "trailing-comma": "all"
  },
  "stylelint": {
    "rules": {
      "at-rule-empty-line-before": [
        "always",
        {
          "except": [
            "first-nested"
          ]
        }
      ],
      "block-closing-brace-empty-line-before": "never",
      "block-no-empty": true,
      "color-hex-case": "lower",
      "color-named": "always-where-possible",
      "color-no-invalid-hex": true,
      "comment-no-empty": true,
      "custom-property-empty-line-before": [
        "always",
        {
          "except": [
            "after-custom-property",
            "first-nested"
          ]
        }
      ],
      "declaration-block-no-duplicate-properties": true,
      "declaration-no-important": true,
      "function-url-quotes": "always",
      "indentation": 2,
      "number-leading-zero": "always",
      "number-no-trailing-zeros": true,
      "max-empty-lines": 1,
      "media-feature-name-case": "lower",
      "media-feature-name-no-unknown": true,
      "media-feature-name-no-vendor-prefix": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "no-unknown-animations": true,
      "property-case": "lower",
      "property-no-unknown": true,
      "property-no-vendor-prefix": true,
      "rule-empty-line-before": [
        "always-multi-line",
        {
          "except": [
            "first-nested"
          ]
        }
      ],
      "selector-max-empty-lines": 0,
      "selector-pseudo-class-case": "lower",
      "selector-pseudo-class-no-unknown": true,
      "selector-pseudo-class-parentheses-space-inside": "never",
      "selector-pseudo-element-case": "lower",
      "selector-pseudo-element-colon-notation": "single",
      "selector-pseudo-element-no-unknown": true,
      "selector-type-case": "lower",
      "selector-type-no-unknown": true,
      "string-quotes": "single",
      "unit-case": "lower",
      "unit-no-unknown": true,
      "value-keyword-case": "lower",
      "value-list-max-empty-lines": 0,
      "value-list-comma-space-after": "always",
      "value-list-comma-space-before": "never",
      "value-no-vendor-prefix": true
    }
  }
}
